{{ form_start(form) }}
    {{ form_row(form.nazwa) }}
    <ul class="tags">
        {# iterate over each existing tag and render its only field: name #}
        {% for uzytkownik in form.users %}
            <li>{{ form_row(uzytkownik.imie) }}</li>
        {% endfor %}
    </ul>
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}