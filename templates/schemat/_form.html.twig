<script type="text/javascript" src="../../js/jquery-2.1.3.min.js"></script>
        <script type="text/javascript" src="../../js/jquery-ui.js"></script>
        <script type="text/javascript" src="../../js/dodUsDawka.js"></script>
{% form_theme form.dawki 'form/czasy_dawki_poziomo_theme.html.twig' %}
        
{{ form_start(form) }}
    {% for pole in form %}
        {% if pole == form.dawki %}
            <ul class="ul_DodajUsun" data-prototype="{{ form_widget(form.dawki.vars.prototype)|e('html_attr') }}">
            {% use "bootstrap_3_layout.html.twig" %}
            {% for dawka in pole %}
                <li>
                {{ form_row(dawka) }}
                </li>
            {% endfor %}
            </ul>
        {% else %}
           {{ form_row(pole) }}
        {% endif %}
    {% endfor %}
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
